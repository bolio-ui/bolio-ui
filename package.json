{
  "name": "@bolio-ui/core",
  "version": "1.1.7",
  "description": "Amazing, modern and creative tools for React UI. 🥷🏼",
  "keywords": [
    "bolio",
    "bolio ui",
    "components",
    "modern components",
    "react components",
    "react ui"
  ],
  "homepage": "https://bolio-ui.com/",
  "bugs": {
    "url": "https://github.com/bolio-ui/bolio-ui/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bolio-ui/bolio-ui"
  },
  "license": "MIT",
  "author": {
    "name": "Bruno Andrade",
    "email": "brunnoandradi@gmail.com"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "/dist",
    "/esm"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "export": "yarn build && next export -o _static",
    "build:storybook": "build-storybook -s ./public",
    "build:clear": "rm -rf ./dist ./esm",
    "build:rollup": "rollup -c scripts/rollup.config.js",
    "build:babel": "babel --no-babelrc --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./core --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
    "build:after": "node scripts/move-built-in.js",
    "build:webpack": "webpack --config scripts/webpack.config.js",
    "build:types": "ttsc -p ./scripts & ttsc -p ./scripts --outDir ./esm",
    "build:package": "yarn build:clear && yarn build:rollup && yarn build:babel && yarn build:types && yarn build:after",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next _static node_modules && yarn install && yarn build",
    "release": "yarn build && yarn build:package && yarn publish --access public --non-interactive"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.6.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^11.0.1",
    "algoliasearch": "^4.14.2",
    "classnames": "^2.3.1",
    "gray-matter": "^4.0.3",
    "jest-styled-components": "^7.0.5",
    "kbar": "0.1.0-beta.6",
    "next": "12.3.1",
    "next-seo": "^5.5.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-device-detect": "^1.17.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-dom": "^6.12.0",
    "react-live": "^2.2.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-join-line": "^1.0.2",
    "remark-autolink-headings": "^6.0.1",
    "remark-slug": "^6.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@bolio-ui/icons": "^1.0.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^15.5.4",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "plop": "^2.7.4",
    "prettier": "2.3.2",
    "rollup": "^2.64.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "styled-jsx": "4.0.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
